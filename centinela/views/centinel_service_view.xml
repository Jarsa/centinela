<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
<record id="centinel_service_form" model="ir.ui.view">
	<field name="name">Service</field>
	<field name="model">centinel.service</field>
	<field name="arch" type="xml">
		<form string="Service">
			<sheet>
				<group colspan="4">
					<group colspan="2" col="4">
						<field name="service"/>
						<field name="mix"/>
						<field name="group"/>
						<field name="turn"/>
						<field name="subgroup"/>
						<field name="rol"/>
					</group>
				</group>
			</sheet>
		</form>
	</field>
</record>
<record id="centinel_service_tree" model="ir.ui.view">
	<field name="name">Service</field>
	<field name="model">centinel.service</field>
	<field name="arch" type="xml">
		<tree string="Service">
			<field name="service"/>
			<field name="mix"/>
			<field name="group"/>
			<field name="turn"/>
			<field name="subgroup"/>
			<field name="rol"/>
		</tree>
	</field>
</record>
<record id="action_centinel_service" model="ir.actions.act_window">
	<field name="name">Service</field>
	<field name="res_model">centinel.service</field>
	<field name="view_type">form</field>
	<field name="view_mode">form, tree</field>
</record>
</data>
</odoo>
