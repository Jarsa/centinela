<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		<record id="centinel_subgroup_form" model="ir.ui.view">
			<field name="name">Subgroup</field>
			<field name="model">centinel.subgroup</field>
			<field name="arch" type="xml">
				<form string="Subgroup">
					<sheet>
						<h1>
							<field name='name' nolabel='1' placeholder='1'/>
						</h1>
						<notebook>
							<page string='Rol'>
								<field name='rol_ids' nolabel='1'/>
							</page>
						</notebook>
					</sheet>
				</form>
			</field>
		</record>
		<record id="centinel_subgroup_tree" model="ir.ui.view">
			<field name="name">Subgroup</field>
			<field name="model">centinel.subgroup</field>
			<field name="arch" type="xml">
				<tree string="Vacancy post">
					<field name='name'/>
				</tree>
			</field>
		</record>
		<record id="action_centinel_subgroup" model="ir.actions.act_window">
			<field name="name">Group</field>
			<field name="res_model">centinel.subgroup</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
		</record>
	</data>
</odoo>